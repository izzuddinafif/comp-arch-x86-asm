; add your code here
DATA SEGMENT
PORTA EQU 00H
PORTB EQU 02H
PORTC EQU 04H
PORT_CON EQU 06H
DATA ENDS
CODE SEGMENT
MOV AX,DATA
MOV DS, AX

ORG 0000H
START:

MOV DX, PORT_CON
MOV AL, 10000000B
OUT DX, AL


MOV AL,00H
MOV CX,10H


XX:

PUSH CX
MOV DX, PORTA
OUT DX,AL
INC AL
MOV CX, 09F36H
D1: LOOP D1
POP CX

LOOP XX

JMP START

CODE ENDS
END



